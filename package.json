{
  "name": "strapi-plugin-media-browser",
  "version": "0.0.1",
  "sideEffects": false,
  "description": "A convenient way to browse, manage and select all your Strapi assets, inspired by MacOS.",
  "keywords": [
    "strapi",
    "media",
    "browser",
    "assets",
    "library",
    "tags",
    "files",
    "images",
    "videos"
  ],
  "strapi": {
    "name": "Media Browser",
    "description": "A convenient way to browse, manage and select all your Strapi assets, inspired by MacOS.",
    "kind": "plugin"
  },
  "files": [
    "dist",
    "strapi-admin",
    "strapi-server"
  ],
  "dependencies": {
    "@radix-ui/react-accordion": "^1.1.1",
    "@radix-ui/react-dialog": "^1.0.3",
    "@radix-ui/react-dropdown-menu": "^2.0.4",
    "@radix-ui/react-toolbar": "^1.0.3",
    "@react-spring/web": "^9.7.1",
    "@reduxjs/toolkit": "^1.9.3",
    "@strapi/design-system": "^1.6.6",
    "@strapi/helper-plugin": "^4.0.0",
    "@strapi/icons": "^1.6.6",
    "@strapi/utils": "^4.0.0",
    "@vitejs/plugin-react": "^3.1.0",
    "axios": "^1.3.4",
    "fs-extra": "^11.1.0",
    "koa-range": "^0.3.0",
    "koa-static": "^5.0.0",
    "lodash": "^4.17.21",
    "mime-types": "^2.1.35",
    "nanoid": "3.3.4",
    "react-dropzone": "^14.2.3",
    "react-helmet": "^6.1.0",
    "react-redux": "8.0.5",
    "react-virtuoso": "^4.0.3",
    "sonner": "^0.3.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.0",
    "@commitlint/config-conventional": "^17.4.4",
    "@strapi/database": "^4.0.0",
    "@strapi/strapi": "^4.0.0",
    "@strapi/typescript-utils": "^4.0.0",
    "@types/fs-extra": "^11.0.1",
    "@types/koa-bodyparser": "^4.3.10",
    "@types/koa-range": "^0.3.2",
    "@types/koa-static": "^4.0.2",
    "@types/mime-types": "^2.1.1",
    "@types/react": "^17.0.53",
    "@types/react-dom": "^17.0.18",
    "@types/react-helmet": "^6.1.6",
    "@types/react-router-dom": "^5.3.3",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "commitlint": "^17.4.4",
    "concurrently": "^7.6.0",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "koa": "^2.14.1",
    "lint-staged": "^13.2.0",
    "prettier": "^2.8.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-is": "^17.0.2",
    "react-router-dom": "^5.3.4",
    "styled-components": "^5.3.9",
    "typescript": "4.9.5",
    "vite": "^4.2.0"
  },
  "peerDependencies": {
    "@strapi/strapi": ">=4.0.0",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-is": "^17.0.0",
    "react-router-dom": "^5.0.0",
    "styled-components": "^5.0.0"
  },
  "author": "Josh Ellis (https://github.com/joshuaellis)",
  "engines": {
    "node": ">=14.19.1 <=18.x.x",
    "npm": ">=6.0.0"
  },
  "scripts": {
    "dev": "rm -rf dist && concurrently \"yarn dev:admin\" \"yarn dev:server\"",
    "dev:admin": "vite build --watch",
    "dev:server": "tsc -p tsconfig.server.json -w",
    "clean": "cd demo && yarn clean && cd .. && rm -rf dist node_modules",
    "build": "rm -rf dist && yarn build:admin && yarn build:server",
    "build:admin": "vite build",
    "build:server": "tsc -p tsconfig.server.json",
    "lint": "TIMING=1 eslint \"**/*.ts*\"",
    "prepare": "husky install",
    "prettier:write": "prettier --write .",
    "prettier:check": "prettier --check .",
    "test": "yarn test:ts",
    "test:ts": "yarn test:ts:admin && yarn test:ts:server",
    "test:ts:admin": "tsc -p tsconfig.json --noEmit",
    "test:ts:server": "tsc -p tsconfig.server.json --noEmit"
  },
  "license": "MIT",
  "lint-staged": {
    "*.{md,yaml,yml}": [
      "prettier --check"
    ],
    "*.{js,ts,tsx}": [
      "eslint"
    ]
  },
  "packageManager": "yarn@3.4.1"
}
